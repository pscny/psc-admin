<div class="page-header">
  <h1>Log in</h1>
</div>
<%= simple_form_for @admin, :url => sessions_path, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :email %>
  <%= f.input :password %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary', :value => 'Sign In' %>
  </div>
<% end %>
